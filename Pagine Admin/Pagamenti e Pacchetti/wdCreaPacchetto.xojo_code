#tag Events cbDurata
	#tag Event
		Sub ValueChanged()
		  txtScadenza.Enabled = cbDurata.Value
		  cmdApriCalendario.Enabled = cbDurata.Value
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events cmdCrea
	#tag Event
		Sub Pressed()
		  //Controlli
		  if txtNome.Text="" then
		    MsgBox "The name field is empty."
		    txtNome.SetFocus
		    exit sub
		  end if
		  
		  if txtCosto.Text="" then
		    MsgBox "The cost field is empty."
		    txtCosto.SetFocus
		    exit sub
		  end if
		  
		  if IsNumeric(txtCosto.Text) = False then
		    MsgBox "The cost must be a numeric field."
		    txtCosto.SetFocus
		    exit sub
		  end if
		  
		  if txtFavorPoint.Text="" then
		    MsgBox "The Favor Point field is empty."
		    txtFavorPoint.SetFocus
		    exit sub
		  end if
		  
		  if IsNumeric(txtFavorPoint.Text) = False then
		    MsgBox "The Favor Point must be a numeric field."
		    txtFavorPoint.SetFocus
		    exit sub
		  end if
		  
		  if immagineCorrente = Nil then
		    MsgBox "Insert an image for the package."
		    Exit Sub
		  end if
		  
		  if connettiDB then
		    //Uppo l'immagine
		    if UppaImmagineInserimento(immagineCorrente, imgTmp) = False Then
		      Exit sub
		    end if
		    
		    dim sql As String
		    sql = "INSERT INTO USD_package(name, cost, favor_point, description, has_duration, expiration, image_path, image_name) VALUES('"+Apici(txtNome.Text)
		    sql = sql + "', "+txtCosto.Text + ","+txtFavorPoint.Text+",'"+Apici(txtDescrizione.Text)+"','"+str(cbDurata.Value)+"','"+TornaDataSQL(txtScadenza.Text)+"','"+CartellaUpload.Name+"', '" + immagineCorrente.Name +"')"
		    db.SQLExecute(sql)
		    if db.Error Then
		      MsgBox ErroreDatabase
		      Exit sub
		    Else
		      MsgBox "The package has been created successfull."
		      AggiornalstPacchetti(lstwdPacchetti)
		      Self.Close
		    end if
		  end if
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events cmdApriCalendario
	#tag Event
		Sub Pressed()
		  if Session.wdCalendarioAperta = False then
		    dim t As new wdCalendario
		    t.txtData = txtScadenza
		    t.Show
		  end if
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events fuImmagini
	#tag Event
		Sub FileAdded(Filename As String, Bytes as UInt64, MimeType as String)
		  //Rimuovo qualsiasi file che non sia un'immagine
		  If Right(Filename, 4) <> ".jpg" And Right(Filename, 4) <> ".png" Then
		    MsgBox("Please choose only JPG or PNG files.")
		    Me.RemoveFileAtIndex(Me.FileCount - 1)
		    exit sub
		  End If
		  
		  me.Upload
		End Sub
	#tag EndEvent
	#tag Event
		Sub UploadFinished(Files() As WebUploadedFile)
		  //Imposto immagineCorrente
		  dim uploadFolder As FolderItem = CartellaUpload
		  immagineCorrente = uploadFolder.Child(Files(0).Name)
		  
		  if immagineCorrente.Exists then
		    MsgBox "There are already a file called '"+immagineCorrente.Name+"'"
		    Exit sub
		  end if
		  
		  imgTmp = new WebPicture(Files(0).Data, Files(0).Name)
		  ivAnteprima.Picture = imgTmp
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events cmdAnnulla
	#tag Event
		Sub Pressed()
		  if immagineCorrente <> nil then
		    if immagineCorrente.Exists Then
		      immagineCorrente.Delete
		    end if
		  end if
		  
		  Self.Close
		End Sub
	#tag EndEvent
#tag EndEvents
