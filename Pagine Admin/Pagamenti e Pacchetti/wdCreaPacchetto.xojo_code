#tag WindowCode
	#tag Event
		Sub Close()
		  if Session.Available then
		    Session.wdCreaPacchettoAperto = False
		  end if
		End Sub
	#tag EndEvent

	#tag Event
		Sub Open()
		  if Session.Available then
		    Session.wdCreaPacchettoAperto = True
		  end if
		  
		  CentraWebDialog(self)
		End Sub
	#tag EndEvent


	#tag Property, Flags = &h0
