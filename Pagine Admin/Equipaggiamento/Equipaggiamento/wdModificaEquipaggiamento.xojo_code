#tag Events pmCategorie
	#tag Event
		Sub Opening()
		  me.AddRow("--Select a category--")
		  me.ListIndex = 0
		  
		  dim rs As RecordSet
		  dim i as integer
		  rs = ElencoCategorie
		  if rs <> Nil then
		    for i = 0 to rs.RecordCount-1
		      me.AddRow(DeApici(rs.IdxField(2).StringValue))
		      me.RowTag(i+1) = rs.IdxField(1).Value
		      rs.MoveNext
		    next
		  end if
		End Sub
	#tag EndEvent
