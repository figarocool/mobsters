	#tag Event
		Sub Close()
		  if Session.Available then
		    Session.wdModificaArmaAperto = False
		  end if
		End Sub
	#tag EndEvent

	#tag Event
		Sub Open()
		  if Session.Available then
		    Session.wdModificaArmaAperto = True
		  end if
		  
		  CentraWebDialog(self)
		End Sub
	#tag EndEvent

	#tag Event
		Sub Shown()
		  if connettiDB then
		    dim sql As String
		    sql = "SELECT name, description, attack, defense, cost, visible, level_required, quantity, idcategory, image_path, avatar_image , image_name, cost_FP "
		    sql = sql + "FROM equipment WHERE idequipment="+lstwdArmi.RowTag(lstwdArmi.ListIndex)
		    dim rs As RecordSet
		    rs = db.SQLSelect(sql)
		    if db.Error Then
		      MsgBox ErroreDatabase
		      exit sub 
		    end if
		    //Inserisco le informazioni di base dell'arma
		    txtNome.Text = DeApici(rs.IdxField(1).StringValue)
		    txtDescrizione.Text = DeApici(rs.IdxField(2).StringValue)
		    txtAttacco.Text = rs.IdxField(3).Value
		    txtDifesa.Text = rs.IdxField(4).Value
		    txtCosto.Text = FormattaNumeri(rs.IdxField(5).StringValue)
		    cbVisibile.Value = rs.IdxField(6).Value
		    txtLivelloRichiesto.Text = rs.IdxField(7).Value
		    
		    if rs.IdxField(8).Value <> -1 then
		      txtDisponibilità.Text = rs.IdxField(8).Value
		    Else
		      cbIllimitata.Value = True
		    end if
		    
		    cbImmagineAvatar.Value = rs.IdxField(11).Value
		    txtFP.Text = rs.IdxField(13).StringValue
		    
		    //Seleziono e vedo l'immagine
		    if rs.IdxField(10).StringValue <> "" then
		      immaginePrecedente = dir.Child("upload").Child(rs.IdxField(10).StringValue).Child(rs.IdxField(12).StringValue)
		      immagineCorrente = immaginePrecedente
		      if immaginePrecedente <> nil Then
		        if immaginePrecedente.Exists Then
		          imgTmp = new WebPicture(immaginePrecedente)
		          //Apro l'immagine
		          ivAnteprima.Picture = imgTmp
		        end if
		      end if
		    end if
		    
		    //Imposto la categoria
		    dim i As Integer
		    for i=0 to pmCategorie.ListCount-1 
		      if pmCategorie.RowTag(i)=rs.IdxField(9).Value then
		        pmCategorie.ListIndex = i
		        exit for
		      end if
		    next
		    
		    //Seleziono le caratteristiche avanzate se ce ne sono ed in tal caso le inserisco
		    sql = "SELECT attack_modifier, attack_percentage, defense_modifier, defense_percentage, cash_modifier, cash_percentage, xp_modifier, xp_percentage, damage_modifier, damage_percentage, "
		    sql = sql + "health_modifier, health_percentage, stamina_modifier, stamina_percentage"+EndOfLine
		    sql = sql + "FROM equipment_additional_characterist WHERE idequipment="+lstwdArmi.RowTag(lstwdArmi.ListIndex)
		    rs = db.SQLSelect(sql)
		    if db.Error Then
		      MsgBox ErroreDatabase
		      exit sub
		    end if
		    //Visualizzo nella web dialog
		    if rs.RecordCount > 0 Then
		       txtModAttacco.Text = rs.IdxField(1).Value
		      cbModAttacco.Value = rs.IdxField(2).Value
		      txtModDifesa.Text = rs.IdxField(3).Value
		      cbModDifesa.Value = rs.IdxField(4).Value
		      txtModCash.Text = rs.IdxField(5).Value
		      cbModCash.Value = rs.IdxField(6).Value
		      txtModXP.Text = rs.IdxField(7).Value
		      cbModXP.Value = rs.IdxField(8).Value
		      txtModDanni.Text = rs.IdxField(9).Value
		      cbModDanni.Value = rs.IdxField(10).Value
		      txtModSalute.Text = rs.IdxField(11).Value
		      cbModSalute.Value = rs.IdxField(12).Value
		      txtModStamina.Text = rs.IdxField(13).Value
		      cbModStamina.Value = rs.IdxField(14).Value
		    end if
		    
		    //Seleziono il mantenimento se presente
		    sql = "SELECT upkeep FROM equipment_upkeep WHERE idequipment="+lstwdArmi.RowTag(lstwdArmi.ListIndex)
		    rs = db.SQLSelect(sql)
		    if db.Error Then
		      MsgBox ErroreDatabase
		      exit sub
		    end if
		    //Visualizzo nella web dialog
		    if rs.RecordCount > 0 Then
		      txtMantenimento.Text = FormattaNumeri(rs.IdxField(1).Value)
		    end if
		    
		  Else
		    //Errore nell'apertura del db
		    MsgBox ErroreDatabase
		    exit sub
		  end if
		End Sub
	#tag EndEvent


	#tag Method, Flags = &h0
