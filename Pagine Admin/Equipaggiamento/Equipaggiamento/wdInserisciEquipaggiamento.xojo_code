#tag Events cmdCrea
	#tag Event
		Sub Pressed()
		  //Controlli
		  if txtNome.Text = "" then
		    MsgBox "Insert a valid name for the equipment."
		    txtNome.SetFocus
		    exit sub
		  end if
		  
		  if txtAttacco.Text = "" then
		    MsgBox "Insert a valid attack for the equipment."
		    txtAttacco.SetFocus
		    exit sub
		  end if
		  
		  if IsNumeric(txtAttacco.Text) = False then
		    MsgBox "The attack must be a number."
		    txtAttacco.SetFocus
		    exit sub
		  end if
		  
		  if txtDifesa.Text = "" then
		    MsgBox "Insert a valid attack for the equipment."
		    txtDifesa.SetFocus
		    exit sub
		  end if
		  
		  if IsNumeric(txtDifesa.Text) = False then
		    MsgBox "The defense must be a number."
		    txtDifesa.SetFocus
		    exit sub
		  end if
		  
		  if txtCosto.Text = "" then
		    MsgBox "Insert a valid attack for the equipment."
		    txtCosto.SetFocus
		    exit sub
		  end if
		  
		  if IsNumeric(RimuoviFormattazioneNumeri(txtCosto.Text)) = False then
		    MsgBox "The cost must be a number."
		    txtCosto.SetFocus
		    exit sub
		  end if
		  
		  if txtLivelloRichiesto.Text <> "" then
		    if IsNumeric(txtLivelloRichiesto.Text) = False then
		      MsgBox "The required level must be a number."
		      txtLivelloRichiesto.SetFocus
		      exit sub
		    end if
		  end if
		  
		  if txtDisponibilità.Text <> "" then
		    if IsNumeric(txtDisponibilità.Text) = False then
		      MsgBox "The availability must be a number."
		      txtDisponibilità.SetFocus
		      exit sub
		    end if
		  end if
		  
		  if pmCategorie.ListIndex = -1 or pmCategorie.ListIndex = 0 then
		    MsgBox "Select a valid category"
		    pmCategorie.SetFocus
		    exit sub
		  end if
		  
		  if txtFP.Text <> "" then
		    if IsNumeric(txtFP.Text) = False then
		      MsgBox "The FP must be a number."
		      txtModAttacco.SetFocus
		      exit sub
		    end if
		  end if
		  
		  if txtModAttacco.Text <> "" then
		    if IsNumeric(txtModAttacco.Text) = False then
		      MsgBox "The attack modifier must be a number."
		      txtModAttacco.SetFocus
		      exit sub
		    end if
		  end if
		  
		  if txtModDifesa.Text <> "" then
		    if IsNumeric(txtModDifesa.Text) = False then
		      MsgBox "The defense modifier must be a number."
		      txtModDifesa.SetFocus
		      exit sub
		    end if
		  end if
		  
		  if txtModDanni.Text <> "" then
		    if IsNumeric(txtModDanni.Text) = False then
		      MsgBox "The damage modifier must be a number."
		      txtModDanni.SetFocus
		      exit sub
		    end if
		  end if
		  
		  if txtModCash.Text <> "" then
		    if IsNumeric(txtModCash.Text) = False then
		      MsgBox "The cash modifier must be a number."
		      txtModCash.SetFocus
		      exit sub
		    end if
		  end if
		  
		  if txtModXP.Text <> "" then
		    if IsNumeric(txtModXP.Text) = False then
		      MsgBox "The experience modifier must be a number."
		      txtModXP.SetFocus
		      exit sub
		    end if
		  end if
		  
		  if txtModSalute.Text <> "" then
		    if IsNumeric(txtModSalute.Text) = False then
		      MsgBox "The health modifier must be a number."
		      txtModSalute.SetFocus
		      exit sub
		    end if
		  end if
		  
		  if txtModStamina.Text <> "" then
		    if IsNumeric(txtModStamina.Text) = False then
		      MsgBox "The stamina modifier must be a number."
		      txtModStamina.SetFocus
		      exit sub
		    end if
		  end if
		  
		  if txtMantenimento.Text <> "" then
		    if IsNumeric(RimuoviFormattazioneNumeri(txtMantenimento.Text)) = False then
		      MsgBox "The upkeep field must be a number."
		      txtMantenimento.SetFocus
		      exit sub
		    end if
		  end if
		  
		  if immagineCorrente = Nil then
		    MsgBox "Insert an image for the equipment."
		    exit sub
		  end if
		  
		  
		  if connettiDB then
		    //Salvo l'immagine sul server
		    if UppaImmagineInserimento(immagineCorrente, imgTmp)=False Then
		      exit sub
		    end if
		    
		    //Inserisco informazioni base dell'arma nel db
		    dim sql As String
		    sql = "INSERT INTO equipment(name, attack, defense, cost, visible, level_required, quantity, description, idcategory, image_path, image_name, avatar_image, cost_FP) VALUES ('"
		    sql = sql + Apici(txtNome.Text) + "', "+ txtAttacco.Text + ", "+ txtDifesa.Text + ", " + RimuoviFormattazioneNumeri(txtCosto.Text)
		    sql = sql + ", '" + str(cbVisibile.Value) + "', " + txtLivelloRichiesto.Text +", " +controlloDisponibilità + ", '" + Apici(controlloDescrizione) + "', " + pmCategorie.RowTag(pmCategorie.ListIndex)+",'"+CartellaUpload.Name+"', '"+immagineCorrente.Name + "', '"+ str(cbImmagineAvatar.Value) + "','"+txtFP.Text+"')"
		    //Eseguo query e controllo per errori
		    db.SQLExecute(sql)
		    if db.Error then
		      MsgBox ErroreDatabase
		      exit sub
		    end if
		    
		    dim idArma As Integer
		    idArma = db.LastRowID
		    
		    //Controllo  se ci sono Caratteristi EXTRA ed in tal caso le inserisco nel db
		    if  txtModAttacco.Text<>"" OR txtModDifesa.Text<>"" OR txtModCash.Text<>"" OR txtModXP.Text<>"" OR txtModDanni.Text<>"" OR txtModSalute.Text<>"" OR txtModStamina.Text<>""Then
		      sql = "INSERT INTO equipment_additional_characterist(idequipment, attack_modifier, attack_percentage, defense_modifier, defense_percentage, "
		      sql = sql + "cash_modifier, cash_percentage, xp_modifier, xp_percentage, damage_modifier, damage_percentage, health_modifier, health_percentage, stamina_modifier, stamina_percentage) VALUES ("+Str(idArma)+",'"+ txtModAttacco.Text+"','"+str(cbModAttacco.Value)+"','"+txtModDifesa.Text
		      sql = sql + "', '"+str(cbModDifesa.Value)+"','"+txtModCash.Text+"','"+str(cbModCash.Value)+"','"+txtModXP.Text+"','"+str(cbModXP.Value)+"','"+txtModDanni.Text+"','"+str(cbModDanni.Value)+"','"+txtModSalute.Text+"','"+str(cbModSalute.Value)+"','"+txtModStamina.Text+"','"+str(cbModStamina.Value)+"')"
		      //Eseguo query e controllo per errori
		      db.SQLExecute(sql)
		      if db.Error then
		        MsgBox ErroreDatabase
		        exit sub
		      end if
		    end if
		    
		    //Controllo se vi è un mantenimento per l'arma, in tal caso la inserisco nel db
		    if txtMantenimento.Text<>"" Then
		      sql = "INSERT INTO equipment_upkeep(idequipment, upkeep) VALUES("+str(idArma)+","+RimuoviFormattazioneNumeri(txtMantenimento.Text)+")"
		      //Eseguo query e controllo per errori
		      db.SQLExecute(sql)
		      if db.Error then
		        MsgBox ErroreDatabase
		        exit sub
		      end if
		    end if
		    
		    AggiornaLstArmi(lstwdArmi)
		    
		    if cbImmagineAvatar.Value Then
		      if Session.wdInserisciImmaginiAvatarAperto = False then
		        dim t As new wdInserisciImmaginiAvatar
		        t.idEquipaggiamento = idArma
		        t.Show
		      end if
		    Else
		      MsgBox "The equipment has been added with successful." 
		    end if
		    
		    Self.Close
		    
		  end if
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events pmCategorie
	#tag Event
		Sub Opening()
		  me.AddRow("--Select a category--")
		  me.ListIndex = 0
		  
		  dim rs As RecordSet
		  dim i as integer
		  rs = ElencoCategorie
		  if rs <> Nil then
		    for i = 0 to rs.RecordCount-1
		      me.AddRow(DeApici(rs.IdxField(2).StringValue))
		      me.RowTag(i+1) = rs.IdxField(1).Value
		      rs.MoveNext
		    next
		  end if
		End Sub
