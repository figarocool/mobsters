	#tag Event
		Sub Close()
		  if Session.Available then
		    Session.wdInserisciArmaAperto = False
		  end if
		End Sub
	#tag EndEvent

	#tag Event
		Sub Open()
		  if Session.Available then
		    Session.wdInserisciArmaAperto = True
		  end if
		  
		  CentraWebDialog(self)
		End Sub
	#tag EndEvent


	#tag Method, Flags = &h0
		Function controlloDescrizione() As String
		  if txtDescrizione.Text = "" then
		    Return ""
		  Else
		    return txtDescrizione.Text
		  end if
		End Function
	#tag EndMethod

	#tag Method, Flags = &h0
		Function controlloDisponibilità() As String
		  if cbIllimitata.Value then
		    Return "-1"
		  Else
		    Return txtDisponibilità.Text
		  end if
		End Function
	#tag EndMethod


