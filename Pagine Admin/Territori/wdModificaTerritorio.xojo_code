		Sub Close()
		  if Session.Available then
		    Session.wdModificaTerritorioAperto = False
		  end if
		End Sub
	#tag EndEvent

	#tag Event
		Sub Open()
		  if Session.Available then
		    Session.wdModificaTerritorioAperto = True
		  end if
		  
		  CentraWebDialog(self)
		End Sub
	#tag EndEvent

	#tag Event
		Sub Shown()
		  if connettiDB=False Then
		    exit sub
		  end if
		  
		  'Carico informazioni di base
		  dim sql As String
		  sql = "SELECT name, cost, income, level_required, idcategory, image_path, image_name FROM place WHERE idplace="+lstwdTerritori.RowTag(lstwdTerritori.ListIndex)
		  dim rs As RecordSet
		  rs = db.SQLSelect(sql)
		  if db.Error Then
		    MsgBox ErroreDatabase
		    exit sub
		  end if
		  
		  'Seleziono il tipo
		  for i as Integer = 0 to pmTipo.ListCount-1
		    if pmTipo.RowTag(i)=rs.IdxField(5).Value then
		      pmTipo.ListIndex = i
		      exit for i
		    end if
		  next
		  
		  txtNome.Text = DeApici(rs.IdxField(1).StringValue)
		  txtCosto.Text = FormattaNumeri(rs.IdxField(2).Value)
		  txtEntrate.Text = FormattaNumeri(rs.IdxField(3).Value)
		  txtLivello.Text = rs.IdxField(4).Value
		  
		  //Seleziono l'immagine e poi la carico in una Picture
		  immaginePrecedente = dir.Child("upload").Child(rs.IdxField(6).StringValue).Child(rs.IdxField(7).StringValue)
		  immagineCorrente = immaginePrecedente
		  if immaginePrecedente <> nil Then
		    if immaginePrecedente.Exists Then
		      imgTmp = new WebPicture(immaginePrecedente)
		      //Apro l'immagine
		      ivAnteprima.Picture = imgTmp
		    end if
		  end if
		  
		  'Territorio advanced
		  if rs.IdxField(5).Value = 2 Then
		    sql = "SELECT health_max_var, stamina_max_var, energy_max_var, cash_var, attack_var, defense_var FROM place_advanced WHERE idplace="+lstwdTerritori.RowTag(lstwdTerritori.ListIndex)
		    rs = db.SQLSelect(sql)
		    if db.Error then
		      MsgBox ErroreDatabase
		      exit sub
		    end if
		    txtSalute.Text = rs.IdxField(1).StringValue
		    txtStamina.Text = rs.IdxField(2).StringValue
		    txtEnergia.Text = rs.IdxField(3).StringValue
		    txtCash.Text = rs.IdxField(4).StringValue
		    txtAttacco.Text = rs.IdxField(5).StringValue
		    txtDifesa.Text = rs.IdxField(6).StringValue
		    rs.Close
		    exit sub
		  end if
		  
		  'Territorio Development
		  if rs.IdxField(5).Value = 3 Then
		    sql = "SELECT idplace_buildon FROM place_development WHERE idplace="+lstwdTerritori.RowTag(lstwdTerritori.ListIndex)
		    rs = db.SQLSelect(sql)
		    if db.Error then
		      MsgBox ErroreDatabase
		      exit sub
		    end if
		    for i as Integer = 0 to pmCostruitoSu.ListCount-1
		      if pmCostruitoSu.RowTag(i)=rs.IdxField(1).Value then
		        pmCostruitoSu.ListIndex=i
		        exit for i
		      end if
		    next
		    rs.Close
		    exit sub
		  end if
		End Sub
	#tag EndEvent


