#tag Events cmdModifica
	#tag Event
		Sub Pressed()
		  dim territorioBase As Integer = 1
		  dim territorioAdvanced As Integer = 2
		  dim territorioDevelopment As Integer = 3
		  
		  //Controlli
		  if txtNome.Text = "" then
		    MsgBox "The Name field is empty."
		    txtNome.SetFocus
		    exit sub
		  end if
		  
		  if txtCosto.Text="" then
		    MsgBox "The Cost field is empty."
		    txtCosto.SetFocus
		    exit sub
		  end if
		  
		  if IsNumeric(RimuoviFormattazioneNumeri(txtCosto.Text)) = False then
		    MsgBox "The Cost must be a numeric field."
		    txtCosto.SetFocus
		    exit sub
		  end if
		  
		  if txtEntrate.Text="" then
		    MsgBox "The Income field is empty."
		    txtEntrate.SetFocus
		    exit sub
		  end if
		  
		  if IsNumeric(RimuoviFormattazioneNumeri(txtEntrate.Text)) = False then
		    MsgBox "The Income must be a numeric field."
		    txtEntrate.SetFocus
		    exit sub
		  end if
		  
		  'Il livello non è obbligatorio per i sistemi advanced
		  if pmTipo.RowTag(pmTipo.ListIndex) <> territorioAdvanced Then
		    if txtLivello.Text="" then
		      MsgBox "The level field is empty."
		      txtLivello.SetFocus
		      exit sub
		    end if
		    
		    if IsNumeric(txtLivello.Text) = False then
		      MsgBox "The level must be a numeric field."
		      txtLivello.SetFocus
		      exit sub
		    end if
		  end if
		  
		  if pmTipo.ListIndex = territorioAdvanced then
		    if txtSalute.Text<>"" then
		      if IsNumeric(txtSalute.Text) = False then
		        MsgBox "The health variation must be a numeric field."
		        txtSalute.SetFocus
		        exit sub
		      end if
		    end if
		    
		    if txtStamina.Text<>"" then
		      if IsNumeric(txtStamina.Text) = False then
		        MsgBox "The Stamina variation must be a numeric field."
		        txtStamina.SetFocus
		        exit sub
		      end if
		    end if
		    
		    if txtEnergia.Text<>"" then
		      if IsNumeric(txtEnergia.Text) = False then
		        MsgBox "The energy variation must be a numeric field."
		        txtEnergia.SetFocus
		        exit sub
		      end if
		    end if
		    
		    if txtCash.Text<>"" then
		      if IsNumeric(txtCash.Text) = False then
		        MsgBox "The cash variation must be a numeric field."
		        txtCash.SetFocus
		        exit sub
		      end if
		    end if
		    
		    if txtAttacco.Text<>"" then
		      if IsNumeric(txtAttacco.Text) = False then
		        MsgBox "The attack variation must be a numeric field."
		        txtAttacco.SetFocus
		        exit sub
		      end if
		    end if
		    
		    if txtDifesa.Text<>""then
		      if IsNumeric(txtDifesa.Text) = False then
		        MsgBox "The defense variation must be a numeric field."
		        txtDifesa.SetFocus
		        exit sub
		      end if
		    end if
		  end if
		  
		  if pmTipo.RowTag(pmTipo.ListIndex) = territorioDevelopment then
		    if pmCostruitoSu.ListIndex=0 OR pmCostruitoSu.ListIndex=-1 Then
		      MsgBox "Select a valid place in 'Build On' field."
		      Exit sub
		    end if
		  end if
		  
		  if immagineCorrente = nil then
		    MsgBox "Insert an image for the place."
		    exit sub
		  end if
		  
		  //Inizio ad inserire il tutto
		  if connettiDB Then
		    dim idTerritorio As String
		    idTerritorio = lstwdTerritori.RowTag(lstwdTerritori.ListIndex)
		    
		    if UppaImmagineModifica(immagineCorrente, imgTmp, immaginePrecedente) = False Then
		      exit sub
		    end if
		    
		    db.SQLExecute("BEGIN TRANSACTION")
		    
		    'Aggiorno informazioni base
		    dim sql As String
		    sql = "UPDATE place SET name='"+Apici(txtNome.Text)+"', level_required="+txtLivello.Text+", cost="+RimuoviFormattazioneNumeri(txtCosto.Text)+", income="+RimuoviFormattazioneNumeri(txtEntrate.Text)+", idcategory='"+pmTipo.RowTag(pmTipo.ListIndex)+"', image_path='"+CartellaUpload.Name+"', image_name='"+immagineCorrente.Name + "'"
		    sql = sql + " WHERE idplace="+idTerritorio
		    
		    db.SQLExecute(sql)
		    if db.Error Then
		      MsgBox ErroreDatabase
		      db.Rollback
		      db.SQLExecute("END TRANSACTION")
		      exit sub
		    end if
		    
		    'Territorio basic
		    if pmTipo.ListIndex = territorioBase then
		      MsgBox "The Place has been modified with successuful."
		    end if
		    
		    'Elimino informazioni di più
		    sql = "DELETE FROM place_development WHERE idplace="+idTerritorio+";"
		    sql = sql +"DELETE FROM place_advanced WHERE idplace="+idTerritorio+";"
		    db.SQLExecute(sql)
		    if db.Error Then
		      MsgBox ErroreDatabase
		      db.Rollback
		      db.SQLExecute("END TRANSACTION")
		      exit sub
		    end if
		    
		    'Territorio advanced
		    If pmTipo.RowTag(pmTipo.ListIndex) = territorioAdvanced then
		      //Info avanzate
		      sql = "INSERT INTO place_advanced(idplace, health_max_var, stamina_max_var, energy_max_var, cash_var, attack_var, defense_var) VALUES("
		      sql = sql + str(idTerritorio) + ",'"+txtSalute.Text+"','"+txtStamina.Text+"','"+txtEnergia.Text+"','"+txtCash.Text+"','"+txtAttacco.Text+"','"+txtDifesa.Text+"')"
		      db.SQLExecute(sql)
		      if db.Error Then
		        MsgBox ErroreDatabase
		        db.Rollback
		        db.SQLExecute("END TRANSACTION")
		        exit sub
		      end if
		      MsgBox "The Place has been modified with successuful."
		    end if
		    
		    'Territorio Development
		    if pmTipo.RowTag(pmTipo.ListIndex) = territorioDevelopment then
		      sql = "INSERT INTO place_development(idplace, idplace_buildon) VALUES("
		      sql = sql + str(idTerritorio) + ","+pmCostruitoSu.RowTag(pmCostruitoSu.ListIndex)+")"
		      db.SQLExecute(sql)
		      if db.Error Then
		        MsgBox ErroreDatabase
		        db.Rollback
		        db.SQLExecute("END TRANSACTION")
		        exit sub
		      End If
		      MsgBox "The Place has been modified with successuful."
		    end if
		  end if
		  
		  Aggiornalst(lstwdTerritori)
		  self.Close
		End Sub
	#tag EndEvent
