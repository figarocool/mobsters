#tag Events label_Picker
	#tag Event
		Sub MouseDown(X As Integer, Y As Integer, Details As REALbasic.MouseEvent)
		  'Un jour a-t-il t cliqu ?
		  if label_Picker(index).Text="" then            'Est-il valide ?
		    return
		  end if
		  '___________________________________________
		  
		  JourSelection = val(label_Picker(index).Text)   'On affecte le jour slectionn.
		  
		  DateSelection.day = JourSelection
		  DateSelection.Year = AnneeSelection
		  DateSelection.Month = MoisSelection
		  
		  if index < MoisPrecedent Then
		    
		    If MoisSelection = 1 Then DateSelection.Year = AnneeSelection -1
		    DateSelection.Month = MoisSelection - 1
		    
		    if ScrollBar1.Value>ScrollBar1.Minimum then
		      ScrollBar1.Value=ScrollBar1.Value-1
		    end if
		    
		  Elseif index > MoisSuivant Then
		    
		    If now.Month = 12 Then now.Year = AnneeSelection +1
		    DateSelection.Month = DateSelection.Month + 1
		    
		    if ScrollBar1.Value>ScrollBar1.Minimum then
		      ScrollBar1.Value=ScrollBar1.Value+1
		    end if
		    
		  Else
		    
		    Meth_WebCalendarPicker_draw  'On raffraichie le calendrier
		    
		  end if
		  
		  
		  
		  
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events Label_Month_Prev
	#tag Event
		Sub MouseDown(X As Integer, Y As Integer, Details As REALbasic.MouseEvent)
		  if ScrollBar1.Value>ScrollBar1.Minimum then
		    ScrollBar1.Value=ScrollBar1.Value-1
		  end if
		  
		  
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events Label_Month_Next
	#tag Event
		Sub MouseDown(X As Integer, Y As Integer, Details As REALbasic.MouseEvent)
		  if ScrollBar1.Value<ScrollBar1.Maximum then
		    ScrollBar1.Value=ScrollBar1.Value+1
		  end if
		  
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events ScrollBar1
	#tag Event
		Sub ValueChanged()
		  Meth_WebCalendarPicker_draw
		  
		End Sub
	#tag EndEvent
