#tag WindowCode
	#tag Event
		Sub Open()
		  me.Height=218
		  me.width=218
		  
		  ' On dfini ici en fonction du language du navigateur le texte contenu dans les label_picker()
		  
		  dim Lang as string = Session.Header("Accept-Language")
		  
		  if instr(lang, "de")>0 then
		    
		    mois=Array("","Januar","Februar", "Mrz", "April", "Mai", "Juni", "Juli", "August", "September", "Oktober", "November", "Dezember")
		    jours = array("","Son","Mon","DIe","Mit","Don","Fre","Sam")
		    
		  elseif instr(lang, "fr")>0 then
		    
		    mois=Array("","Janvier","Fvrier","Mars","Avril","Mai","Juin","Juillet","Aot","Septembre","Octobre","Novembre","Dcembre")
		    jours = array("","Dim","Lun","Mar","Mer","Jeu","Ven","Sam")
		    
		  elseif instr(lang, "it")>0 then
		    
		    mois=Array("","Gennaio","Febbraio","Marzo","Aprile","Maggio","Giugno","Luglio","Agosto","Settembre","Ottobre","Novembre","Dicembre")
		    jours = array("","Dom","Lun","Mar","Mer","Gio","Ven","Sab")
		    
		  else
		    mois=Array("","January","February","March","April","May","June","July","August","September","October","November","December")
		    jours = array("","Sun","Mon","Tue","Wen","Thu","Fri","Sat")
		    
		  end if
		  
		  Label_day1.Text = jours(1)
		  Label_day2.Text = jours(2)
		  Label_day3.Text = jours(3)
		  Label_day4.Text = jours(4)
		  Label_day5.Text = jours(5)
		  Label_day6.Text = jours(6)
		  Label_day7.Text = jours(7)
		  
		  
		  'Aujourd'hui
		  
		  now=new Date
		  DateDuJour = new Date
		  DateSelection = new Date
		  
		  'Slction du mois en cours
		  ScrollBar1.Value=12*(now.year-1980) + now.month-1
		  
		  JourSelection = now.day
		  MoisSelection = now.Month
		  AnneeSelection = now.Year
		  
		  ' On dessine le calendrier
		  Meth_WebCalendarPicker_draw
		End Sub
	#tag EndEvent


	#tag Method, Flags = &h0
		Sub Meth_WebCalendarPicker_draw()
		  dim etiquette as integer
		  dim lbl_jour_precedent as integer
		  dim lbl_jour_actuel as integer = 0
		  dim lbl_jour_suivant as integer
		  dim JourActuel as integer
		  dim PremierJour as integer
		  dim JourInitialSet as integer
		  dim PremierJourSett as integer
		  dim JoudDuMois as integer
		  dim mois as integer
		  dim annee as integer
		  PremierJourSett = 1  ' si 0 : On affiche Directemment le mois mme si le 1er est un dimanche, si 1 : On affiche la semaine du mois qui prcde
		  
		  'Aujourd'hui
		  JourActuel = JourSelection
		  
		  'Effacement de tout les numros
		  for etiquette=0 to 41
		    label_Picker(etiquette).Text=""
		  next
		  
		  PremierJour = 1
		  mois = 1+ScrollBar1.Value mod 12
		  annee = 1980+ScrollBar1.Value\12
		  
		  'mettre la date de la barre de dfilement
		  now.day = PremierJour
		  now.Month = mois
		  now.Year = annee
		  
		  'affichage mois anne
		  lblMonth.Text=mois(now.month) + " " + str(now.year)
		  
		  'affichage du calendrier
		  JourInitialSet = now.DayOfWeek - 1 - PremierJourSett
		  if JourInitialSet = -1 Then JourInitialSet = 7
		  lbl_jour_actuel = JourInitialSet
		  
		  '============mois actuel=================
		  do
		    label_Picker(lbl_jour_actuel).Text = str(now.Day)                         'Affichage des tiquettes
		    
		    If DateDuJour.Day = now.Day and DateDuJour.Month=mois And DateSelection.Day<>now.Day Then
		      
		      label_Picker(lbl_jour_actuel).Style=Style_TodayDate                                 ' On affiche avec le style date du jour (rouge)
		      
		    ElseIf DateSelection.day = now.Day and DateSelection.Month=mois Then
		      
		      label_Picker(lbl_jour_actuel).Style=StylePickerDayNr_Selected                  ' On affiche avec le style non selectionn (gris)
		      
		    else
		      label_Picker(lbl_jour_actuel).Style=StylePickerDayNr                                 ' On affiche avec le style date du jour (bleu)
		      
		    end if
		    
		    now.day = now.day+1                                                         'Jour suivant
		    lbl_jour_actuel = lbl_jour_actuel+1                                      'Etiquette suivante
		  loop until now.day=1
		  
		  
		  '============mois precedent=================
		  if JourInitialSet > 0 Then
		    
		    now.day = PremierJour
		    if mois = 1 Then now.Year = annee-1
		    now.Month = mois-1
		    JoudDuMois = 0
		    MoisPrecedent = JourInitialSet
		    
		    do
		      now.day=now.day+1                           
		      JoudDuMois=JoudDuMois+1                  
		    loop until now.day=1
		    
		    for lbl_jour_precedent = 0 to JourInitialSet -1 
		      label_Picker(JourInitialSet-1-lbl_jour_precedent).Text=str(JoudDuMois)                 
		      label_Picker(JourInitialSet-1-lbl_jour_precedent).Style=StylePickerDayNr_Prev      
		      JoudDuMois= JoudDuMois -1
		    next lbl_jour_precedent
		  end if
		  
		  '============mois suivant=================
		  
		  
		  
		  if lbl_jour_actuel < 41 Then
		    
		    now.day = PremierJour
		    if mois = 12 Then now.Year = annee+1
		    now.Month = mois+1
		    JoudDuMois = 1
		    MoisSuivant = lbl_jour_actuel-1
		    
		    
		    for lbl_jour_suivant = lbl_jour_actuel to 41
		      label_Picker(lbl_jour_suivant).Text=str(JoudDuMois)                       
		      label_Picker(lbl_jour_suivant).Style=StylePickerDayNr_Next           
		      JoudDuMois= JoudDuMois +1
		    next lbl_jour_suivant
		    
		  end if
		  
		  now.Month = now.Month -1
		  
		  JourSelection = JourActuel
		  MoisSelection = mois
		  AnneeSelection = annee
		  
		  now.Day = JourSelection
		  now.Month = MoisSelection
		  now.Year = AnneeSelection
		  Label_Date.Text=now.ShortDate
		End Sub
	#tag EndMethod


