	#tag Method, Flags = &h0
		Function ControlloEsistenzaPremioTramiteID(idPremio As Integer) As Boolean
		  dim i As Integer
		  for i =0 to lstPremi.RowCount-1
		    if lstPremi.RowTag(i)=idPremio then
		      Return True
		    end if
		  next
		  Return False
		End Function
