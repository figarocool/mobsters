		Sub Pressed()
		  if lstPremi.ListIndex <> -1 then
		    //Seleziono l'arma dal db
		    dim sql As String
		    sql = "SELECT idequipment, name FROM equipment WHERE idequipment="+lstPremi.RowTag(lstPremi.ListIndex)
		    if connettiDB then
		      dim rs As RecordSet
		      rs = db.SQLSelect(sql)
		      if db.Error then
