		Sub AggiungiArma()
		  dim sql As String
		  sql = lstArmi.RowTag(lstArmi.ListIndex)
		  sql = "SELECT idequipment, name FROM equipment WHERE idequipment="+lstArmi.RowTag(lstArmi.ListIndex)
		  if connettiDB then
		    dim rs As RecordSet
		    rs = db.SQLSelect(sql)
		    if db.Error then
		      MsgBox ErroreDatabase
		      exit sub
		    end if
		    
		    dim idarma As Integer
		    idarma=rs.IdxField(1).Value
		    //Aggiungo alla listbox giusta
		    Select case rgPremi.SelectedCell.Left
		      //Aggiunta alla list box dei premi normli
		    case 0
		      //Aggiungo alla Listbox l'arma con quantità, visibilità e se è Random o meno
		      lstPremi.AddRow rs.IdxField(2).StringValue
		      lstPremi.RowTag(lstPremi.LastIndex) = idarma
		      lstPremi.Cell(lstPremi.LastIndex, 1) = txtQuantità.Text
		      lstPremi.Cell(lstPremi.LastIndex, 2) = "EQP"
		      lstPremi.Cell(lstPremi.LastIndex, 3) = str(cbVisibile.Value)
		      lstPremi.Cell(lstPremi.LastIndex, 4) = str(cbRandom.Value)
		      rs.Close
		      //Aggiungo alla Listbox dei premi di fine missione
		    Case 1
		      //Aggiungo alla Listbox l'arma con quantità, visibilità e se è Random o meno
		      lstPremiFineMissione.AddRow rs.IdxField(2).StringValue
		      lstPremiFineMissione.RowTag(lstPremiFineMissione.LastIndex) = idarma
		      lstPremiFineMissione.Cell(lstPremiFineMissione.LastIndex, 1) = txtQuantità.Text
		      lstPremiFineMissione.Cell(lstPremiFineMissione.LastIndex, 2) = "EQP"
		      lstPremiFineMissione.Cell(lstPremiFineMissione.LastIndex, 3) = str(cbVisibile.Value)
		      lstPremiFineMissione.Cell(lstPremiFineMissione.LastIndex, 4) = str(cbRandom.Value)
		      rs.Close
		    End Select
		  end if
		  
		  txtQuantità.Text = "1"
		End Sub
	#tag EndMethod

	#tag Method, Flags = &h0
		Sub AggiungiTerritorio()
		  dim sql As String
		  sql = "SELECT idplace, name FROM place WHERE idplace="+lstArmi.RowTag(lstArmi.ListIndex)
		  if connettiDB=False then
		    exit sub
		  end if
		  
		  dim rs As RecordSet
		  rs = db.SQLSelect(sql)
		  if db.Error then
		    MsgBox ErroreDatabase
		    exit sub
		  end if
		  
		  dim idTerritorio As Integer
		  idTerritorio=rs.IdxField(1).Value
		  'Aggiungo alla Listbox l'arma e la quantità
		  Select case rgPremi.SelectedCell.Left
		    'Ho selezionato il premio per click 
		  Case 0
		    lstPremi.AddRow rs.IdxField(2).StringValue
		    lstPremi.RowTag(lstPremi.LastIndex) = idTerritorio
		    lstPremi.Cell(lstPremi.LastIndex, 1) = txtQuantità.Text
		    lstPremi.Cell(lstPremi.LastIndex, 2) = "PLC"
		    lstPremi.Cell(lstPremi.LastIndex, 3) = str(cbVisibile.Value)
		    lstPremi.Cell(lstPremi.LastIndex, 4) = str(cbRandom.Value)
		    rs.Close
		    'E' un premio di fine misssione
		  case 1
		    lstPremiFineMissione.AddRow(rs.IdxField(2).StringValue)
		    lstPremiFineMissione.RowTag(lstPremiFineMissione.LastIndex) = idTerritorio
		    lstPremiFineMissione.Cell(lstPremiFineMissione.LastIndex, 1) = txtQuantità.Text
		    lstPremiFineMissione.Cell(lstPremiFineMissione.LastIndex, 2) = "PLC"
		    lstPremiFineMissione.Cell(lstPremiFineMissione.LastIndex, 3) = str(cbVisibile.Value)
		    lstPremiFineMissione.Cell(lstPremiFineMissione.LastIndex, 4) = str(cbRandom.Value)
		  End Select
		  
		  txtQuantità.Text = "1"
		End Sub
