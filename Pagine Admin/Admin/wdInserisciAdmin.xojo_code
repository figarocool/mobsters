#tag Events cmdCrea
	#tag Event
		Sub Pressed()
		  //Controlli
		  if txtNomeAdmin.Text ="" Then
		    MsgBox "Insert a valid name for the admin."
		    txtNomeAdmin.SetFocus
		    exit sub
		  end if
		  
		  if txtPassword.Text="" Then
		    MsgBox "The password field is empty."
		    txtPassword.SetFocus
		    exit sub
		  end if
		  
		  if txtConfermaPassword.Text="" Then
		    MsgBox "The confirm password field is empty."
		    txtConfermaPassword.SetFocus
		    exit sub
		  end if
		  
		  if txtPassword.Text <> txtConfermaPassword.Text then
		    MsgBox "The two passwords are different."
		    txtConfermaPassword.SetFocus
		    exit sub
		  end if
		  
		  if txtEmail.Text ="" then
		    MsgBox "The e-mail field is empty."
		    txtEmail.SetFocus
		    exit sub
		  end if
		  
		  if InStr(txtEmail.Text,"@") = 0 then
		    MsgBox "Please note that the e-mail field must contain the @."
		    txtEmail.SetFocus
		    exit sub
		  end if
		  
		  if txtConfermaEmail.Text ="" then
		    MsgBox "The confirm e-mail field is empty."
		    txtConfermaEmail.SetFocus
		    exit sub
		  end if
		  
		  if InStr(txtConfermaEmail.Text,"@") = 0 then
		    MsgBox "Please note that the 'Confirm e-mail' field must contain the @."
		    txtConfermaEmail.SetFocus
		    exit sub
		  end if
		  
		  if txtEmail.Text <> txtConfermaEmail.Text then
		    MsgBox "The two e-mail addresses are different."
		    txtConfermaEmail.SetFocus
		    exit sub
		  end if
		  
		  //Controllo se c'è un utente o admin con quel nome
		  dim sql As String
		  sql = "SELECT COUNT(name) FROM mobster WHERE name='"+Apici(txtNomeAdmin.Text)+"'"
		  if connettiDB then
		    dim rs As RecordSet
		    rs = db.SQLSelect(sql)
		    if rs.IdxField(1).Value = 0 then
		      sql = "SELECT COUNT(name) FROM admin WHERE name='"+Apici(txtNomeAdmin.Text)+"'"
		      rs = db.SQLSelect(sql)
		      if rs.IdxField(1).Value <> 0 Then
		        MsgBox "There is already an admin with this name, please change the name to continue."
		        exit sub
		      end if
		    Else
		      MsgBox "There is already a user with this name, please change the name to continue."
		      exit sub
		    end if
		  end if
		  
		  //Creo Insert per inserire nel db
		  sql = "INSERT INTO admin(name, password, equipment_access, place_access, mission_access, payment_access, comunication_access, avatar_access, user_access, admin_access, actived ,email) VALUES"
		  sql = sql +"('"+ Apici(txtNomeAdmin.Text) + "','"+EncodeHex(MD5(txtPassword.Text)) + "','"+str(cbAccessoArmi.Value)+"','"+str(cbTerritori.Value)+"','"+str(cbMissioni.Value)+"','"+str(cbCash.Value)
		  sql = sql + "','"+str(cbComunicazioni.Value)+"','" + str(cbAvatar.Value) + "', '"+str(cbUtenti.Value)+"','"+str(cbAdmin.Value)+"','"+TornaStatoBooleano(cbAttivo)+"','"+txtEmail.Text+"')"
		  
		  //Eseguo Insert
		  if connettiDB then
		    db.SQLExecute(sql)
		    if db.Error Then
		      MsgBox ErroreDatabase
		      exit sub
		    end if
		    MsgBox "Admin has been created successfully."
		  end if
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events cmdSelezionaTutti
