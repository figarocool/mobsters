		Sub Pressed()
		  cbAccessoArmi.Value = True
		  cbCash.Value = True
		  cbComunicazioni.Value = True
		  cbMissioni.Value = True
		  cbTerritori.Value = True
		  cbUtenti.Value = True
		  cbAdmin.Value = True
		  cbAvatar.Value = True
		End Sub
	#tag EndEvent
