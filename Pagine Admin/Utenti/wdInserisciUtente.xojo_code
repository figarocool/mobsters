#tag WindowCode
	#tag Event
		Sub Close()
		  if Session.Available then
		    Session.wdInserisciUtenteAperto = False
		  end if
		End Sub
	#tag EndEvent

	#tag Event
		Sub Open()
		  if Session.wdInserisciUtenteAperto then
		    exit sub
		  end if
		  
		  if Session.Available then
		    Session.wdInserisciUtenteAperto = True
		  end if
		  
		  CentraWebDialog(self)
		End Sub
	#tag EndEvent


