#tag Events cmdModifica
	#tag Event
		Sub Pressed()
		  //Controlli
		  if txtNomeMobster.Text = "" then
		    MsgBox "Insert a valid name."
		    txtNomeMobster.SetFocus
		    Exit sub
		  end if
		  
		  //Livello
		  if txtLivello.Text = "" then
		    MsgBox "Insert a valid level."
		    txtLivello.SetFocus
		    exit sub
		  end if
		  
		  if IsNumeric(txtLivello.Text) = False then
		    MsgBox "The level must be a number."
		    txtLivello.SetFocus
		    exit sub
		  end if
		  
		  //Esperienza
		  if txtEsperienza.Text = "" then
		    MsgBox "Insert a valid experience."
		    txtEsperienza.SetFocus
		    exit sub
		  end if
		  
		  if IsNumeric(txtEsperienza.Text) = False then
		    MsgBox "The experience must be a number."
		    txtEsperienza.SetFocus
		    exit sub
		  end if
		  
		  //Password
		  if txtPassword.Text = "" then
		    MsgBox "Insert a valid password."
		    txtPassword.SetFocus
		    exit sub
		  end if
		  
		  if txtConfermaPassword.Text = "" then
		    MsgBox "Confirm the password."
		    txtConfermaPassword.SetFocus
		    exit sub
		  end if
		  
		  if txtPassword.Text <> txtConfermaPassword.Text then
		    MsgBox "The two password must be equal."
		    txtPassword.SetFocus
		    exit sub
		  end if
		  
		  //Email
		  if txtEmail.Text = ""Then
		    MsgBox "Insert a valid mail."
		    txtEmail.SetFocus
		    exit sub
		  end if
		  
		  if InStr(txtEmail.Text, "@") = 0 then
		    MsgBox "Email must contain the '@' character."
		    txtEmail.SetFocus
		    exit sub
		  end if
		  
		  if txtConfermaEmail.Text = ""Then
		    MsgBox "Confirm the email."
		    txtConfermaEmail.SetFocus
		    exit sub
		  end if
		  
		  if InStr(txtConfermaEmail.Text, "@") = 0 then
		    MsgBox "Email must contain the '@' character."
		    txtConfermaEmail.SetFocus
		    exit sub
		  end if
		  
		  if txtEmail.Text <> txtConfermaEmail.Text then
		    MsgBox "The two emails must be equal."
		    exit sub
		  end if
		  
		  //Salute corrente
		  if txtSaluteCorrente.Text = "" then
		    MsgBox "Insert a valid health."
		    txtSaluteCorrente.SetFocus
		    exit sub
		  end if
		  
		  if IsNumeric(txtSaluteCorrente.Text) = False then
		    MsgBox "The health must be a number."
		    txtSaluteCorrente.SetFocus
		    exit sub
		  end if
		  //Stamina corrente
		  if txtStaminaCorrente.Text = "" then
		    MsgBox "Insert a valid stamina."
		    txtStaminaCorrente.SetFocus
		    exit sub
		  end if
		  
		  if IsNumeric(txtStaminaCorrente.Text) = False then
		    MsgBox "The stamina must be a number."
		    txtStaminaCorrente.SetFocus
		    exit sub
		  end if
		  //Energia corrente
		  if txtEnergiaCorrente.Text = "" then
		    MsgBox "Insert a valid energy."
		    txtEnergiaCorrente.SetFocus
		    exit sub
		  end if
		  
		  if IsNumeric(txtEnergiaCorrente.Text) = False then
		    MsgBox "The energy must be a number."
		    txtEnergiaCorrente.SetFocus
		    exit sub
		  end if
		  //Attacco
		  if txtAttacco.Text = "" then
		    MsgBox "Insert a valid attack."
		    txtAttacco.SetFocus
		    exit sub
		  end if
		  
		  if IsNumeric(txtAttacco.Text) = False then
		    MsgBox "The attack must be a number."
		    txtAttacco.SetFocus
		    exit sub
		  end if
		  //Skill point
		  if txtSkillPoint.Text = "" then
		    MsgBox "Insert a valid skill point value."
		    txtSkillPoint.SetFocus
		    exit sub
		  end if
		  
		  if IsNumeric(txtSkillPoint.Text) = False then
		    MsgBox "The skill point value must be a number."
		    txtSkillPoint.SetFocus
		    exit sub
		  end if
		  //Soldi
		  if txtSoldi.Text = "" then
		    MsgBox "Insert a valid cash value."
		    txtSoldi.SetFocus
		    exit sub
		  end if
		  
		  if IsNumeric(RimuoviFormattazioneNumeri(txtSoldi.Text)) = False then
		    MsgBox "The cash value must be a number."
		    txtSoldi.SetFocus
		    exit sub
		  end if
		  
		  //Salute massima
		  if txtSaluteMax.Text = "" then
		    MsgBox "Insert a valid maximum health."
		    txtSaluteMax.SetFocus
		    exit sub
		  end if
		  
		  if IsNumeric(txtSaluteMax.Text) = False then
		    MsgBox "The maximum health must be a number."
		    txtSaluteMax.SetFocus
		    exit sub
		  end if
		  //Stamina massima
		  if txtStaminaMax.Text = "" then
		    MsgBox "Insert a valid maximum stamina value."
		    txtStaminaMax.SetFocus
		    exit sub
		  end if
		  
		  if IsNumeric(txtStaminaMax.Text) = False then
		    MsgBox "The maximum stamina value must be a number."
		    txtStaminaMax.SetFocus
		    exit sub
		  end if
		  //Energia massima
		  if txtEnergiaMax.Text = "" then
		    MsgBox "Insert a valid maximum energy value."
		    txtEnergiaMax.SetFocus
		    exit sub
		  end if
		  
		  if IsNumeric(txtEnergiaMax.Text) = False then
		    MsgBox "The maximum energy value must be a number."
		    txtEnergiaMax.SetFocus
		    exit sub
		  end if
		  //Difesa
		  if txtDifesa.Text = "" then
		    MsgBox "Insert a valid defense value."
		    txtDifesa.SetFocus
		    exit sub
		  end if
		  
		  if IsNumeric(txtDifesa.Text) = False then
		    MsgBox "The defense value must be a number."
		    txtDifesa.SetFocus
		    exit sub
		  end if
		  //Favor Point
		  if txtFavorPoint.Text = "" then
		    MsgBox "Insert a valid Favor Point value for the equipment."
		    txtFavorPoint.SetFocus
		    exit sub
		  end if
		  
		  if IsNumeric(txtFavorPoint.Text) = False then
		    MsgBox "The Favor Point value must be a number."
		    txtFavorPoint.SetFocus
		    exit sub
		  end if
		  
		  //Soldi banca
		  if txtSoldiBanca.Text = "" then
		    MsgBox "Insert a valid bank cash value."
		    txtSoldiBanca.SetFocus
		    exit sub
		  end if
		  
		  if IsNumeric(RimuoviFormattazioneNumeri(txtSoldiBanca.Text)) = False then
		    MsgBox "The bank cash value must be a number."
		    txtSoldiBanca.SetFocus
		    exit sub
		  end if
		  
		  //HG
		  if txtHG.Text = "" then
		    MsgBox "Insert a valid value for the HGs."
		    txtHG.SetFocus
		    exit sub
		  end if
		  
		  if IsNumeric(txtHG.Text) = False then
		    MsgBox "HG must be a number."
		    txtHG.SetFocus
		    exit sub
		  end if
		  
		  //Immagine
		  if immagineCorrente = nil Then
		    MsgBox "Insert an image for the mobster."
		    Exit sub
		  end if
		  
		  
		  if connettiDB = False then
		    exit sub
		  end if
		  
		  //Uppo l'immagine
		  if UppaImmagineInserimento(immagineCorrente, imgTmp) = False Then
		    Exit sub
		  end if
		  
		  dim sql As String
		  sql = "INSERT INTO mobster(name, level, health_current, health_max, stamina_current, stamina_max, energy_current, energy_max, experience, attack, "
		  sql = sql + "defense, skill_point, favor_point, cash, bank_cash, HG, email, password, visible, image_path, image_name) VALUES('"+Apici(txtNomeMobster.Text) + "', "+txtLivello.Text + ", "+txtSaluteCorrente.Text
		  sql = sql + ", "+txtSaluteMax.Text + ", "+txtStaminaCorrente.Text+", "+txtStaminaMax.Text+", "+txtEnergiaCorrente.Text
		  sql = sql + ", "+txtEnergiaMax.Text + ", "+txtEsperienza.Text+", "+txtAttacco.Text+", "+txtDifesa.Text +", "+txtSkillPoint.Text
		  sql = sql + ", "+txtFavorPoint.Text + ", "+RimuoviFormattazioneNumeri(txtSoldi.Text) + ", "+RimuoviFormattazioneNumeri(txtSoldiBanca.Text) + ", "+txtHG.Text
		  sql = sql + ", '"+txtEmail.Text+"', '"+ EncodeHex(MD5(txtPassword.Text)) + "', '"+str(cbVisibile.Value) + "','"+CartellaUpload.Name +"','"+immagineCorrente.Name + "')"
		  
		  db.SQLExecute(sql)
		  if db.Error then
		    MsgBox ErroreDatabase
		    exit sub
		  end if
		  
		  AggiornalstUtenti(lstwdUtenti)
		  MsgBox "The user has been inserted with successful."
		  self.Close
		End Sub
	#tag EndEvent
