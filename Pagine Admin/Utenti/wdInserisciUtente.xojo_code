		Sub Pressed()
		  //Se è stato premuto su annulla allora non si è salvato nulla, percio elimino l'immagine caricata
		  if immagineCorrente <> nil then
		    if immagineCorrente.Exists Then
		      immagineCorrente.Delete
		    end if
		  end if
		  
		  self.Close
		End Sub
